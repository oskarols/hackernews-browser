{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "react-app",
        "plugin:react/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true,
            "impliedStrict": true
        },
        "ecmaVersion": 12,
        "sourceType": "module",
        "project": ["tsconfig.json"]
    },
    "ignorePatterns": [
        "docs/**",
        "public/**"
    ],
    "plugins": [
        "react",
        "jest",
        "@typescript-eslint"
    ],
    "rules": {
    }
}
